<div id="main-wrapper">
    <!-- Page Preloader -->
    <div id="preloader">
        <div id="status">
            <div class="status-mes"></div>
        </div>
    </div>
    <header class="header">
        <!-- Navigation -->
        <nav class="navbar main-menu" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <ul class="navbar-nav nav">
                        <li><a class="summit_logo" href="#page-top">@ViewBag.Region @ViewBag.Program Summit 2018</a></li>
                    </ul>

                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse navbar-right navbar-main-collapse">
                    <ul class="nav navbar-nav">
                        <!-- Hidden li included to remove active class from about link when scrolled up past about section -->
                        <li class="hidden"><a href="#page-top"></a></li>
                        <li><a class="page-scroll" href="#section-intro">Objectives</a></li>
                        <li><a class="page-scroll" href="#section-speaker">Speakers</a></li>
                        <li><a class="page-scroll" href="#section-agenda">Agenda</a></li>
                        @if (((string)ViewBag.Program).ToLower().Equals("itdp"))
                        {
                            <li><a class="page-scroll" href="#section-graduationprofiles">Graduation</a></li>
                        }
                        @*<li><a class="page-scroll" href="#section-pricing">Traveller's Notes</a></li>
                            <li><a class="page-scroll" href="#section-partner">Links</a></li>*@
                        <li><a class="page-scroll" href="#section-venue">Venue</a></li>
                        <li><a class="page-scroll" href="#section-feedback">Feedback</a></li>
                    </ul>
                </div>
            </div>
            <!-- /.container -->
        </nav>
        <!-- .nav -->
    </header>
    <div class="jumbotron text-center">
        <div class="content">
            <div class="event-date">MAY 08–10, 2018</div>
            <h1>Hello, @ViewBag.Region!!</h1>
            <h1>ARE YOU EXCITED FOR THIS YEAR'S SUMMIT?</h1>
            @if (ViewBag.Program.ToLower() == "itdp")
            {
                <p class="lead">
                    Loads of fun-filled activities, some interesting sessions and cool games.
                    <br />Select your program and region to get started
                </p>
            }
            else
            {
                <p class="lead">Meet fellow leaders and Dell rockstars from across the globe. Scroll down to know more</p>
            }
            <div class="col-md-12">
                <div class="col-md-6 section-divider">
                    <div class="row">
                        <p class="lead">Region</p>
                    </div>

                    <div class="row">
                        @foreach (string Region in ViewBag.Regions)
                        {
                            if (Region.ToLower().Equals(((string)ViewBag.Region).ToLower()))
                            {
                                <div class="col-md-4">
                                    <a href="#" class="region selected-region">@Region</a>
                                </div>
                            }
                            else
                            {
                                <div class="col-md-4">
                                    <a href="#" class="region">@Region</a>
                                </div>
                            }
                        }
                        @*<div class="col-md-4">
                                <a href="#" class="region">INDIA</a>
                            </div>
                            <div class="col-md-4">
                                <a href="#" class="region">AUSTIN</a>
                            </div>
                            <div class="col-md-4">
                                <a href="#" class="region">MALAYSIA</a>
                            </div>*@
                    </div>


                </div>
                <div class="col-md-6">
                    <div class="row">
                        <p class="lead">Program</p>
                    </div>

                    <div class="row">
                        @foreach (string Program in ViewBag.Programs)
                        {
                            if (Program.ToLower().Equals(((string)ViewBag.Program).ToLower()))
                            {
                                <div class="col-md-4">
                                    <a href="#" class="program selected-program">@Program</a>
                                </div>
                            }
                            else
                            {
                                <div class="col-md-4">
                                    <a href="#" class="program">@Program</a>
                                </div>
                            }
                        }
                        @*<div class="col-md-4">
                                <a href="#" class="program">ITDP</a>
                            </div>
                            <div class="col-md-4">
                                <a href="#" class="program">ITLP</a>
                            </div>
                            <div class="col-md-4">
                                <a href="#" class="program">Leaders</a>
                            </div>*@
                    </div>
                </div>


            </div>



            <button type="button" class="btn btn-default btn-lg" data-toggle="modal" data-target="#registrationModal">REGISTER YOURSELF</button>

        </div>
    </div>
    <!-- .Jumbotron-->
    <section id="section-intro" class="section-wrapper about-event">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <h2>OUR OBJECTIVES</h2>
                    <p class="lead">
