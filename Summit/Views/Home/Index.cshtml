    <section id="section-agenda" class="section-wrapper section-agenda">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="section-title" style="margin-bottom:0px">
                        <h1>AGENDA</h1>
                    </div>
                </div>
            </div>
            <div class="row">
                @{
                    int i = 1;
                    foreach (Summit.Controllers.HomeController.EventDay day in ViewBag.Agenda)
                    {
                        <div class="col-md-4">
                            <h3>Day @i</h3>
                            @foreach (Summit.Controllers.HomeController.EventSession session in day.Sessions)
                            {
                                <div class="session">
                                    <time>@session.Time</time>
                                    <h3>@session.Title</h3>
                                </div>
                            }
                            @{
                                i++;
                            }

                        </div>
                                }
                }


            </div>
        </div>
    </section>

